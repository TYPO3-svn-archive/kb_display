<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:kb_display/locallang.xml:pi_cached_listView</sheetTitle>
		</TCEforms>
		<el>
			<field_disableListView>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:kb_display/locallang.xml:pi_cached_listView_disableListView</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</field_disableListView>
			<field_itemsPerPage>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:kb_display/locallang.xml:pi_cached_listView_itemsPerPage</label>
					<config>
						<type>input</type>
						<size>10</size>
						<eval>int</eval>
						<range>
							<lower>0</lower>
							<upper>100000</upper>
						</range>
						<default>30</default>
					</config>
				</TCEforms>
			</field_itemsPerPage>
			<field_showPagebrowser>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:kb_display/locallang.xml:pi_cached_listView_showPagebrowser</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</field_showPagebrowser>
			<field_pagesInBrowser>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:kb_display/locallang.xml:pi_cached_listView_pagesInBrowser</label>
					<config>
						<type>input</type>
						<size>10</size>
						<eval>int</eval>
						<range>
							<lower>1</lower>
							<upper>1000</upper>
						</range>
						<default>5</default>
						<displayCond>FIELD:field_showPagebrowser:REQ:true</displayCond>
					</config>
				</TCEforms>
			</field_pagesInBrowser>
		</el>
	</ROOT>
</T3DataStructure>
